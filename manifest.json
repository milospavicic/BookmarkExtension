{
    "manifest_version": 2,
    "name": "Bookmark++",
    "description": "Fast bookmark",
    "version": "1.0",
    "author": "Milos",
    "browser_action": {
        "default_icon": "bookmark_add.png",
        "default_title": "Have a good day",
        "default_popup": "popup.html"
